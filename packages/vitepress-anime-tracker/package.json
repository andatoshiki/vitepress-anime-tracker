{
  "name": "vitepress-anime-tracker",
  "version": "0.0.1",
  "description": "Web component plugin of Vitepress for showcasing a list of tracked Anime similar to myanimelist!",
  "license": "MIT",
  "homepage": "https://github.com/andatoshiki/vitepress-anime-tracker#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/andatoshiki/vitepress-anime-tracker"
  },
  "keywords": ["Bilibili", "Bangumi", "WebComponent", "vitepress"],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "unpkg": "dist/vitepress-anime-tracker/vitepress-anime-tracker.esm.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "stencil build --dev --watch --serve",
    "generate": "stencil generate",
    "ts-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@stencil/core": "^4.7.0",
    "@types/node": "^20.10.0"
  }
}
